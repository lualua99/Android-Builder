[
  {
    "kernelSource": {
      "name": "msm-4.19-kernel",
      "repo": "https://github.com/lualua99/kernel-msm-4.19.git",
      "branch": "main",  // 请根据实际分支名称修改
      "device": "alioth",       // 请填写对应的设备编号
      "defconfig": "arch/arm64/configs/alioth_user_defconfig"     // 请填写defconfig文件相对路径（相对于arch/arm64/configs或arch/arm/configs）
    },
    "toolchains": [
      {
        "name": "proton-clang",
        "repo": "https://github.com/kdrag0n/proton-clang",
        "branch": "master",
        "binaryEnv": ["./bin"]
      },
      {
        "name": "aarch64-gcc",
        "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/+archive/refs/tags/android-12.1.0_r27.tar.gz",
        "binaryEnv": ["bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",    // 请根据实际架构修改（arm或arm64）
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "proton-clang/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "proton-clang/bin/arm-linux-gnueabi-",
        "LD": "ld.lld"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
      // 如需使用自定义AnyKernel3仓库，可添加以下配置
      // "custom": {
      //   "repo": "你的AnyKernel3仓库地址",
      //   "branch": "对应的分支"
      // }
    },
    "enableKernelSU": false,  // 根据需求设置true或false
    "enableLXC": false        // 根据需求设置true或false
  }
]
