[
    {
        "kernelSource": {
            "name": "msm-4.19-kernel-build",
            "repo": "https://github.com/lualua99/kernel-msm-4.19.git",
            "branch": "master",
            "device": "alioth",
            "defconfig": "arch/arm64/configs/alioth_user_defconfig"
        },
        "toolchains": [
            {
                "name": "zyc_clang-14",
                "repo": "https://github.com/EmanuelCN/zyc_clang-14",
                "branch": "master",
                "binaryEnv": [
                    "./bin"
                ]
            },
            {
                "name": "aarch64-gcc",
                "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/+archive/refs/tags/android-12.1.0_r27.tar.gz",
                "binaryEnv": [
                    "bin"
                ]
            },
            {
                "name": "arm-gcc",
                "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-4.9/+archive/refs/tags/android-12.1.0_r27.tar.gz",
                "binaryEnv": [
                    "bin"
                ]
            }
        ],
        "enableCcache": true,
        "params": {
            "ARCH": "arm64",
            "CC": "clang",
            "externalCommands": {
                "CLANG_TRIPLE": "aarch64-linux-gnu-",
                "CROSS_COMPILE": "aarch64-linux-android-",
                "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
                "LD": "ld.lld",
                "AR": "llvm-ar",
                "NM": "llvm-nm",
                "OBJCOPY": "llvm-objcopy",
                "OBJDUMP": "llvm-objdump",
                "READELF": "llvm-readelf",
                "OBJSIZE": "llvm-size",
                "STRIP": "llvm-strip"
            }
        },
        "AnyKernel3": {
            "use": true,
            "release": true,
            "custom": {
                "repo": "https://github.com/your-username/AnyKernel3",
                "branch": "your-device-branch"
            }
        },
        "enableKernelSU": false,
        "enableLXC": false
    }
]
